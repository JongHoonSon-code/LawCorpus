<Window x:Class="LawCorpus.SplitFile"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LawCorpus"
        mc:Ignorable="d"
        Title="SplitFile" Height="450" Width="800">
    <Grid>
        <DockPanel Margin="10">
            <WrapPanel DockPanel.Dock="Top" Margin="0,0,0,10" Width="374" HorizontalAlignment="Center">
                <Label Name="lblRate" Content="Validation % : " Margin="10, 10, 0, 10"></Label>
                <TextBox x:Name="txtRate" Margin="10" Height="25" Width="35" VerticalContentAlignment="Center" HorizontalAlignment="Center">10</TextBox>
                <Button x:Name="btnOpenFile" Click="btnOpenFile_Click" Content="분리" Height="25" Width="35"/>
            </WrapPanel>
        </DockPanel>
    </Grid>
</Window>
